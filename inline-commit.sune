[{"id":"jbcwown","name":"1 Click GitHub Commit","pinned":false,"avatar":"data:image/webp;base64,","url":"gh://multipleof4/.sune/inline-commit.sune","updatedAt":1758059271260,"settings":{"model":"google/gemini-2.5-pro","temperature":"","top_p":"","top_k":"","frequency_penalty":"","repetition_penalty":"","min_p":"","top_a":"","verbosity":"","reasoning_effort":"default","system_prompt":"You include this sune in the extension.html of any sune you use to code with. <sune src=...\nThen use this system prompt and you will get 1 click button to commit your code inside code blocks:\nIn the preceding line to code blocks, apply this format: \n[org/repo@branch/file](https://github.com/org/repo/blob/branch/file \"Fix/Feat/Refactor/etc: Summarize in less than 50chars your following edit\")","html":"<!--\nSune: GitHub Commit Helper\nVersion: 2.9\n-->\n<div id=\"sune_github_commit\" x-data=\"{v:'2.9'}\" class=\"hidden\"></div>\n<script>\n(()=>{\n  const suneEl=document.getElementById('sune_github_commit');\n  if(!suneEl){console.error(\"Sune container not found.\");return}\n  const SUNE_NAME='[Sune: GitHub Commit Helper]', SUNE_V=suneEl.getAttribute('x-data').match(/v:'(.*?)'/)[1];\n  console.log(`${SUNE_NAME} Initializing v${SUNE_V}`);\n\n  const commitFile=async(btn,owner,repo,branch,path,content,commitMsg)=>{\n    const token=window.USER?.githubToken;\n    if(!token){alert('GitHub token not set in Account Settings > API.');return}\n    \n    const defaultClasses=['bg-slate-100','text-slate-700','hover:bg-slate-200'],\n          successClasses=['bg-green-100','text-green-800'],\n          errorClasses=['bg-red-100','text-red-800'];\n    \n    btn.disabled=true;\n    btn.innerHTML='<span class=\"relative flex h-3 w-3\"><span class=\"animate-ping absolute inline-flex h-full w-full rounded-full bg-sky-400 opacity-75\"></span><span class=\"relative inline-flex rounded-full h-3 w-3 bg-sky-500\"></span></span> Authenticating...';\n\n    const H={'Authorization':`Bearer ${token}`,'Accept':'application/vnd.github.v3+json'};\n    try{\n      const userRes=await fetch('https://api.github.com/user',{headers:H});\n      if(!userRes.ok)throw new Error('Failed to fetch GitHub user. Your token may be invalid or lack user:read scope.');\n      const username=(await userRes.json()).login;\n      btn.innerHTML='<span class=\"relative flex h-3 w-3\"><span class=\"animate-ping absolute inline-flex h-full w-full rounded-full bg-sky-400 opacity-75\"></span><span class=\"relative inline-flex rounded-full h-3 w-3 bg-sky-500\"></span></span> Committing...';\n\n      let targetBranch=branch;\n      const repoCheckRes=await fetch(`https://api.github.com/repos/${owner}/${repo}`,{headers:H});\n\n      if(repoCheckRes.status===404){\n        console.log(`${SUNE_NAME} Repo ${owner}/${repo} not found. Creating empty repo...`);\n        const isOrg=owner.toLowerCase()!==username.toLowerCase();\n        const createRepoUrl=isOrg?`https://api.github.com/orgs/${owner}/repos`:'https://api.github.com/user/repos';\n        \n        const createRepoRes=await fetch(createRepoUrl,{method:'POST',headers:H,body:JSON.stringify({name:repo,private:false,auto_init:false})});\n        if(!createRepoRes.ok){\n          const errorData=await createRepoRes.json();\n          let msg=`Repo creation failed: ${errorData.message||'Unknown API error.'}`;\n          if(createRepoRes.status===401||createRepoRes.status===403)msg=`Permission Denied. For personal repos, token needs 'public_repo' scope. For org repos, it needs 'repo' scope.`;\n          else if(createRepoRes.status===422)msg=`Validation Failed. Repo '${repo}' may already exist or be invalid. (API: ${errorData.message})`;\n          throw new Error(msg);\n        }\n        console.log(`${SUNE_NAME} Empty repo created. First commit will be on branch '${targetBranch}'.`);\n      }else if(!repoCheckRes.ok)throw new Error(`Repo check failed: ${repoCheckRes.statusText}`);\n\n      const CONTENTS_URL=`https://api.github.com/repos/${owner}/${repo}/contents/${path}`;\n      let sha;\n      const fileCheckRes=await fetch(`${CONTENTS_URL}?ref=${targetBranch}`,{headers:H});\n      if(fileCheckRes.ok)sha=(await fileCheckRes.json()).sha;\n      else if(fileCheckRes.status!==404)throw new Error(`GET file failed: ${fileCheckRes.statusText}`);\n\n      const msg=commitMsg?.trim()||(sha?`docs: update ${path.split('/').pop()}`:`feat: add ${path.split('/').pop()}`);\n      const b64c=btoa(unescape(encodeURIComponent(content)));\n      const body={message:msg,content:b64c,sha,branch:targetBranch};\n      \n      const putRes=await fetch(CONTENTS_URL,{method:'PUT',headers:H,body:JSON.stringify(body)});\n      if(!putRes.ok)throw new Error(`PUT file failed: ${(await putRes.json()).message}`);\n      \n      btn.classList.remove(...defaultClasses);btn.classList.add(...successClasses);\n      btn.innerHTML='<i data-lucide=\"check\" class=\"h-3.5 w-3.5\"></i> Success';\n    }catch(err){\n      const errorMsg=err.message||'An unknown error occurred.';\n      alert(`${SUNE_NAME} Commit Failed:\\n\\n${errorMsg}`);\n      btn.classList.remove(...defaultClasses);btn.classList.add(...errorClasses);\n      btn.innerHTML='<i data-lucide=\"x\" class=\"h-3.5 w-3.5\"></i> Failed';\n      setTimeout(()=>{\n        btn.disabled=false;\n        btn.classList.remove(...errorClasses);btn.classList.add(...defaultClasses);\n        btn.innerHTML='<i data-lucide=\"github\" class=\"h-3.5 w-3.5\"></i> Commit';\n        window.lucide?.createIcons();\n      },4e3);\n    }finally{window.lucide?.createIcons()}\n  };\n\n  const processBubble=bubble=>{\n    if(!bubble||bubble.dataset.suneGchProcessed)return;\n    let buttonAdded=false;\n    bubble.querySelectorAll('p:not(:has(button.commit-btn))').forEach(p=>{\n      const link=p.querySelector('a');\n      if(!link)return;\n      const m=link.textContent.trim().match(/^([^\\/]+)\\/([^\\/]+)@([^\\/]+)\\/(.+)$/);\n      if(!m)return;\n      const preEl=p.nextElementSibling;\n      if(preEl?.tagName!=='PRE')return;\n      const code=preEl.querySelector('code')?.innerText;\n      if(!code)return;\n\n      const[_,owner,repo,branch,path]=m,commitMsg=link.title;\n      console.log(`${SUNE_NAME} Match found for ${path.trim()}`);\n      \n      const btn=document.createElement('button');\n      btn.className='commit-btn ml-2 inline-flex items-center gap-1.5 rounded-md bg-slate-100 px-2 py-1 text-xs font-medium text-slate-700 hover:bg-slate-200 transition-colors';\n      btn.innerHTML='<i data-lucide=\"github\" class=\"h-3.5 w-3.5\"></i> Commit';\n      btn.onclick=()=>commitFile(btn,owner,repo,branch,path.trim(),code,commitMsg);\n      p.append(btn);\n      window.lucide?.createIcons();\n      buttonAdded=true;\n    });\n    if(buttonAdded)bubble.dataset.suneGchProcessed='true';\n    return buttonAdded;\n  };\n\n  const scanExisting=()=>document.querySelectorAll('#messages .msg-bubble').forEach(processBubble);\n  \n  const observer=new MutationObserver(mutations=>{for(const m of mutations)for(const node of m.addedNodes)if(node.nodeType===1){const b=node.matches?.('.msg-bubble')?[node]:node.querySelectorAll?.('.msg-bubble');b?.forEach(processBubble)}});\n  \n  // **FIX: Replaced fragile setTimeout with a robust polling mechanism.**\n  const newResponseListener=e=>{\n    const id=e?.detail?.message?.id;\n    if(!id)return;\n    \n    let attempts=0, maxAttempts=8, interval=300;\n    const tryProcess=()=>{\n      attempts++;\n      const bubble=window.getBubbleById(id);\n      if(bubble&&processBubble(bubble)){\n        // Success, button was added, stop polling.\n        return;\n      }\n      if(attempts<maxAttempts){\n        // Failure or bubble not ready, try again.\n        setTimeout(tryProcess, interval);\n      }\n    };\n    setTimeout(tryProcess, 150); // Initial delay before starting to poll.\n  };\n\n  const composerEl=window.el?.composer,chatContainer=window.el?.messages;\n  if(chatContainer&&composerEl){\n    observer.observe(chatContainer,{childList:true,subtree:true});\n    composerEl.addEventListener('sune:newSuneResponse',newResponseListener);\n    scanExisting();\n    console.log(`${SUNE_NAME} Observer and event listener attached. Initial scan complete.`);\n  }\n  \n  suneEl.addEventListener('sune:unmount',()=>{\n    observer.disconnect();\n    if(composerEl)composerEl.removeEventListener('sune:newSuneResponse',newResponseListener);\n    console.log(`${SUNE_NAME} Unmounted.`);\n  });\n})();\n</script>\n","extension_html":"<sune src='https://raw.githubusercontent.com/sune-org/store/refs/heads/main/sync.sune' private></sune>","hide_composer":true,"include_thoughts":false,"json_output":false,"ignore_master_prompt":false,"json_schema":""},"storage":{}}]